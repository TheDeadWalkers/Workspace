package Sample;

import java.util.HashMap;
import java.util.Map;

public class mapDemo {

	public static void main(String[] args) {
		
		Map<Integer, String> hm = new HashMap<>();
		hm.put(1, "akash");
		hm.put(2, "ash");
		hm.put(3, "jaggu");
		
		System.out.println(hm);
		
		System.out.println(hm.get(3));

	}

}
