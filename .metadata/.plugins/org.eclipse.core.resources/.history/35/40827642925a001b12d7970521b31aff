package DDF;

import org.testng.annotations.AfterMethod;
import org.testng.annotations.BeforeMethod;
import org.testng.annotations.Test;

public class testng4 {
	
	@BeforeMethod
	public void beforemethod() {
		System.out.println("Before method in class t4");
	}
	
	@Test
	public void t7() {
		System.out.println("in t7");
	}
	
	@AfterMethod
	public void afteremethod() {
		System.out.println("after method in class t4");
	}
	
	@Test
	public void t8() {
		System.out.println("in t8");
	}

}
