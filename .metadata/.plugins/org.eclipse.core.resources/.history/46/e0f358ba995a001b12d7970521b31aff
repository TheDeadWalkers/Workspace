package TestNG;

import org.testng.annotations.Test;

public class testng1 {

	
	@Test(timeOut = 2000)
	public void xyz() throws InterruptedException {
		Thread.sleep(5000);
		System.out.println("xyz");
	}
	
	@Test
	public void abc() {
		System.out.println("abc");
	}
	
	@Test
	public void mno() {
		System.out.println("mno");
	}
	
	@Test
	public void pqr() {
		System.out.println("pqr");
	}
	
}
